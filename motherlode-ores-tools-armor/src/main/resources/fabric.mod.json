{
  "schemaVersion": 1,
  "id": "motherlode-ores-tools-armor",
  "version": "${version}",
  "name": "Motherlode Ores, Tools, & Armor",
  "description": "Adds several new tiers of ores, tools, weapons and armor.",
  "authors": [
    "Team Motherlode"
  ],
  "contact": {
    "issues": "https://github.com/TeamMotherlode/MotherlodeCore/issues",
    "sources": "https://github.com/TeamMotherlode/MotherlodeCore/motherlode-ores-tools-armor"
  },
  "license": "MIT License",
  "icon": "assets/motherlode-ores-tools-armor/icon.png",

  "environment": "*",
  "entrypoints": {
    "motherlode:init": [
      "motherlode.orestoolsarmor.MotherlodeModule"
    ],
    "client": [
      "motherlode.orestoolsarmor.MotherlodeOresToolsArmorClient"
    ]
  },
  "mixins": [
    "motherlode-ores-tools-armor.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.7.4",
    "fabric": ">=0.25.0",
    "minecraft": "1.17.*",
    "motherlode-base": "*"
  },
  "suggests": {
    "flamingo": "*"
  },
  "custom": {
    "modmenu:parent": "motherlode"
  }
}
